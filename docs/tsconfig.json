{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "types": ["vitepress/client", "node"]
  },
  "include": [
    ".vitepress/**/*",
    "**/*.md",
    "**/*.vue",
    "**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}